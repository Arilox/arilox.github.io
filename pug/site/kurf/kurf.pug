doctype html
html
    include ../../includes/head.pug
    +head(Kurf, "../")
    body
        div(class="column fullscreen spaceBetween")
            div(class="row center bubble")
                a(href="../index.html") 
                    i(class="fas fa-home") 
                h4 > Kurf
            
            div(class="center")
                form(class="row rounded" action="")
                    div(class="select column")
                        div(class="active") 
                            img(class="square50" src="../IMGS/Icons/google.png")
                        div
                            img(class="square50" src="../IMGS/Icons/duckduckgo.png")

                        div
                            img(class="square50" src="../IMGS/Icons/ecosia.png")
                    
                    div
                        input(type="text" autofocus)
                        button(class="")
                            i(class="fas fa-search")

            include ../../includes/footer.pug
        include ../../includes/bodyscripts.pug
        +bodyscripts("../")
