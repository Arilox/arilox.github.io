mixin head(ttl, path)
    head
        meta(charset='utf-8')
        meta(name='viewport' content='width = device-width' initial-scale = '1')
        title= ttl
        script(src='https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js')
        script(src='https://cdn.jsdelivr.net/npm/vue/dist/vue.js')
        
        link(rel='stylesheet' href='https://use.fontawesome.com/releases/v5.8.1/css/all.css' integrity='sha384-50oBUHEmvpQ+1lW4y57PTFmhCaXp0ML5d60M1M7uH2+nqUivzIebhndOJK28anvf' crossorigin='anonymous')
        link(type='text/css' href= path +'style.css' rel='stylesheet')
        link(type='text/css' href= path +'KUSIX/CSS/theme.css' rel='stylesheet')
        link(type='text/css' href= path +'KUSIX/CSS/structure.css' rel='stylesheet')
        link(type='text/css' href= path +'KUSIX/CSS/htmltags.css' rel='stylesheet')
        link(type='text/css' href= path +'KUSIX/CSS/animations.css' rel='stylesheet')
        link(href="https://fonts.googleapis.com/css?family=Lato&display=swap" rel="stylesheet")